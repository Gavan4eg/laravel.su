{
    "name": "laravelrus/laravel.su",
    "description": "The Laravel Framework.",
    "keywords": [
        "laravelrus",
        "laravel.su"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "src/ContentRenderer",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^7.4",
        "laravelrus/content-renderer": "*",
        "graham-campbell/markdown": "^11.1",
        "knplabs/github-api": "^2.12",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "sentry/sentry-laravel": "~1.7",
        "spatie/laravel-backup": "^6.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^2.0",
        "fruitcake/laravel-telescope-toolbar": "^1.1",
        "fzaninotto/faker": "^1.9.1",
        "laravel-frontend-presets/tailwindcss": "^4.1",
        "laravel/telescope": "^2.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpcs": "phpcs --standard=PSR12 --colors --encoding=utf-8 --extensions=php app src tests -p",
        "phpcbf": "phpcbf -s --colors --standard=PSR12 --encoding=utf-8 --extensions=php app src tests"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
